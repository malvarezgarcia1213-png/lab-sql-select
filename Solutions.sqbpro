FROM authors a, titleauthor ta, titles t, publishers p;

SELECT a.au_id, a.au_fname, a.au_lname, p.pub_name, COUNT(*) AS num_titles
FROM authors a, titleauthor ta, titles t, publishers p
GROUP BY a.au_id, p.pub_name;

SELECT a.au_id, a.au_fname, a.au_lname, p.pub_name, COUNT(*) AS qty
FROM authors a, titleauthor ta, titles t, publishers p, sales s
WHERE a.au_id = ta.au_id
  AND ta.title_id = t.title_id
  AND t.pub_id = p.pub_id
  AND s.title_id = t.title_id
GROUP BY a.au_id, p.pub_name
ORDER BY qty DESC
LIMIT 3;

SELECT a.au_id, a.au_fname, a.au_lname, p.pub_name, COUNT(*) AS qty
FROM authors a, titleauthor ta, titles t, publishers p, sales s
WHERE a.au_id = ta.au_id
  AND ta.title_id = t.title_id
  AND t.pub_id = p.pub_id
  AND s.title_id = t.title_id
GROUP BY a.au_id, p.pub_name
ORDER BY qty DESC
LIMIT 23;
</sql><current_tab id="0"/></tab_sql></sqlb_project>
